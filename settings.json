{
  // Vim Settings
  "vim.scroll": 8,
  "vim.leader": ",",
  "vim.sneak": true,
  "vim.foldfix": true,
  "vim.showcmd": true,
  "vim.surround": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.smartcase": true,
  "vim.easymotion": true,
  "vim.autoindent": true,
  "vim.useCtrlKeys": true,
  "vim.overrideCopy": true,
  "vim.sneakReplacesF": true,
  "vim.useSystemClipboard": true,
  "vim.replaceWithRegister": true,
  "vim.neovimUseConfigFile": true,
  "vim.neovimPath": "/snap/bin/nvim",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "black",
  "vim.autoSwitchInputMethod.enable": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.visualstar": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-f>": false,
    "<C-k>": false,
    "<C-l>": false,
    "<C-h>": false,
    "<up>": false,
    "<down>": false
  },
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<C-c>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        ";"
      ],
      "after": [
        "<Esc>",
        // "l"
      ]
    },
    // {
    //   "before": [
    //     "<Esc>"
    //   ],
    //   "after": [
    //     "<Esc>",
    //     "l"
    //   ]
    // }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "/"
      ],
      "commands": [
        "actions.find"
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "commands": [
        "editor.action.startFindReplaceAction"
      ]
    },
    {
      "before": [
        ";"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "f"
      ],
      "after": [
        "s"
      ]
    },
    {
      "before": [
        "F"
      ],
      "after": [
        "S"
      ]
    },
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "workbench.action.files.saveFiles",
        // "eslint.executeAutofix"
      ]
    },
    {
      "before": [
        "<C-]>"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "<C-[>"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "lineBreakInsert"
      ],
      "silent": true
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "p"
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    }
  ],
  "vim.matchpairs": "(:),{:},[:],<:>,>:<",
  "vim.commandLineModeKeyBindings": [
    {
      "before": [
        ":"
      ],
      "commands": [
        {
          "command": "workbench.action.showCommands",
          "args": []
        }
      ]
    },
    {
      "before": [
        ":grep"
      ],
      "commands": [
        "search.action.openNewEditor"
      ]
    }
  ],
  // General Editor Settings
  "editor.codeLens": false,
  "editor.glyphMargin": false,
  "editor.cursorSurroundingLines": 8,
  "editor.autoIndent": "advanced",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnPaste": true,
  "editor.fontFamily": "Consolas",
  "editor.smoothScrolling": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    // "source.fixAll.eslint": "never"
  },
  "editor.scrollbar.horizontalScrollbarSize": 5,
  "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.minimap.maxColumn": 190,
  "editor.minimap.scale": 3,
  "editor.minimap.renderCharacters": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.mouseWheelScrollSensitivity": 3,
  "editor.guides.highlightActiveIndentation": false,
  "editor.inlineCompletionsAccessibilityVerbose": true,
  "editor.minimap.size": "fill",
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": false,
  "editor.renderLineHighlight": "none",
  "editor.fontLigatures": true,
  "editor.wrappingIndent": "deepIndent",
  "editor.wordWrapColumn": 100,
  "editor.minimap.showSlider": "always",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.lineHeight": 20,
  "editor.lineNumbers": "off",
  // Language Specific Settings
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.letterSpacing": -0.7,
    "editor.fontWeight": "100",
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "off",
    "editor.lineHeight": 1.1
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.guides.indentation": false
  },
  // Window Settings
  "window.autoDetectHighContrast": false,
  "window.commandCenter": false,
  "zenMode.showTabs": "none",
  // Terminal Settings
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  // Git and Version Control
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  // Miscellaneous
  "css.validate": false,
  "scss.validate": false,
  "scss.format.spaceAroundSelectorSeparator": true,
  "search.searchOnType": true,
  "zenMode.hideLineNumbers": false,
  "explorer.confirmDragAndDrop": false,
  "html.autoClosingTags": true,
  "html.format.wrapAttributes": "force-expand-multiline",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "search.maxResults": 200,
  "window.title": "${dirty}${activeEditorMedium}",
  "window.titleSeparator": "-",
  "inlineFold.unfoldedOpacity": 1,
  // Extension Specific Settings
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "breadcrumbs.icons": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.symbolPath": "off",
  "typescript.disableAutomaticTypeAcquisition": true,
  "inlineFold.autoFold": true,
  "vim.textwidth": 100,
  "window.zoomPerWindow": false,
  "problems.decorations.enabled": false,
  "diffEditor.renderSideBySide": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "vue.format.template.initialIndent": false,
  "vue.autoInsert.dotValue": true,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 15,
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "left",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizingFixedMinWidth": 60,
  "workbench.list.smoothScrolling": true,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.tabSizingFixedMaxWidth": 180,
  "workbench.editor.autoLockGroups": {
    "jsProfileVisualizer.cpuprofile.table": true
  },
  "http.proxyStrictSSL": false,
  "inlineFold.unfoldOnLineSelect": true,
  "editor.showFoldingControls": "never",
  "editor.inlayHints.enabled": "off",
  "diffEditor.wordWrap": "on",
  "glassit.alpha": 240,
  "editor.letterSpacing": -0.1,
  "editor.fontWeight": "100",
  "security.workspace.trust.enabled": false,
  // "workbench.colorTheme": "Cold Python Theme",
  "files.autoSave": "onFocusChange",
  "editor.stickyScroll.defaultModel": "foldingProviderModel",
  "vim.showmodename": false,
  "gitlens.statusBar.alignment": "left",
  "gitlens.mode.statusBar.alignment": "left",
  "editor.foldingImportsByDefault": false,
  "diffEditor.renderGutterMenu": false,
  "diffEditor.renderIndicators": false,
  "diffEditor.renderMarginRevertIcon": false,
  "workbench.reduceMotion": "off",
  "workbench.remoteIndicator.showExtensionRecommendations": true,
  "workbench.sash.hoverDelay": 100,
  "workbench.tree.renderIndentGuides": "onHover",
  "screencastMode.fontSize": 20,
  "screencastMode.keyboardOptions": {
    "showSingleEditorCursorMoves": false
  },
  "screencastMode.verticalOffset": 0,
  "search.actionsPosition": "right",
  "editor.minimap.enabled": false,
  "window.dialogStyle": "custom",
  "window.confirmBeforeClose": "always",
  "workbench.editor.editorActionsLocation": "hidden",
  "editor.screenReaderAnnounceInlineSuggestion": false,
  "screencastMode.keyboardOverlayTimeout": 500,
  "window.closeWhenEmpty": false,
  "breadcrumbs.symbolSortOrder": "type",
  "workbench.editor.titleScrollbarSizing": "default",
  "workbench.editor.showTabs": "none",
  "workbench.editor.tabSizing": "fixed",
  "window.density.editorTabHeight": "compact",
  "files.autoSaveDelay": 100,
  "eslint.codeActionsOnSave.mode": "all",
  "editor.formatOnSave": true,
  "notebook.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.codeActions.triggerOnFocusChange": true,
  "gitlens.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "typescript.tsserver.maxTsServerMemory": 10072,
  "files.autoSaveWhenNoErrors": true,
  "editor.cursorBlinking": "phase",
  // "searchEverywhere.include": "src/**/*.vue,src/**/*.js,src/**/*.ts,src/**/*.jsx,src/**/*.tsx,src/**/*.json,src/**/*.css,src/**/*.scss",
  // // Minimal excludes
  // "searchEverywhere.exclude": [
  //   "**/node_modules/**",
  //   "**/.git/**",
  //   "**/dist/**"
  // ],
  // // Use extension's exclude mode
  // "searchEverywhere.excludeMode": "search everywhere",
  // // Force re-indexing
  // "searchEverywhere.shouldWorkspaceDataBeCached": false,
  // "searchEverywhere.shouldInitOnStartup": true,
  // Open in the editor area instead of the terminal panel
  // "find-it-faster.general.useTerminalInEditor": false,
  // "find-it-faster.general.killTerminalAfterUse": true,
  // "find-it-faster.general.useTerminalInEditor": false, // bottom panel
  // // "terminal.integrated.defaultLocation": "panel",
  // "find-it-faster.general.killTerminalAfterUse": true,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.allowChords": false,
  "find-it-faster.general.useTerminalInEditor": false,
  "find-it-faster.general.killTerminalAfterUse": true,
  "find-it-faster.findFiles.showPreview": false,
  "window.menuBarVisibility": "toggle",
  "editor.mouseWheelZoom": true,
  "editor.fontSize": 14,
  "window.zoomLevel": 1,
  "debug.enableStatusBarColor": false,
  // "editor.fontSize": 10,
  // "window.zoomLevel": 1,
  // "window.zoomLevel": 2,
  // "window.zoomLevel": -1
  "editor.scrollBeyondLastLine": true,
  // "workbench.colorCustomizations": {
  //   "terminal.background": "#c8c8c8",
  //   "terminal.foreground": "#000000",
  // },
  "workbench.colorTheme": "No Clown Fiesta Theme",
  "workbench.panel.showLabels": false,
  "terminal.integrated.stickyScroll.enabled": true,
  "workbench.activityBar.location": "hidden",
  "terminal.integrated.fontSize": 10,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.mouseWheelZoom": true,
}
